<h1>Dashboard</h1>
<table>
  <tbody>
    <%= link_to("Sign out", destroy_user_session_path, method: :delete) %><br>
    <%= link_to("Destroy", user_registration_path, :confirm => "Are you sure?", method: :delete) %><br><br><br><br><br>

    <% current_user.attributes.slice("email", "spectre_id").each do |k, v| %>
      <%= "#{k}: #{v}\n" %><br>
    <% end %>
    <%= link_to(current_user[:accounts], accounts_path) %><br><br><br><br>

    <%= current_user.attributes %>
  </tbody>
</table>